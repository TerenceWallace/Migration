<GUITemplates>
  <Template Name="template:RoundedButton">
    <Frame Id="NormalState" Config="frame:RoundedButton" Opacity="0.5">
      <Content/>
    </Frame>
    <Frame Id="OverState" Config="frame:RoundedButtonOver" Opacity="0.5">
      <Content/>
    </Frame>
    <Frame Id="DownState" Config="frame:RoundedButtonDown" Opacity="0.5">
      <Content/>
    </Frame>
    <Frame Id="DownOverState" Config="frame:RoundedButtonDownOver" Opacity="0.5">
      <Content/>
    </Frame>
    <Frame Id="DownDisabledState" Config="frame:RoundedButtonDownDisabled" Opacity="0.5">
      <Content/>
    </Frame>
    <Frame Id="NormalDisabledState" Config="frame:RoundedButtonDisabled" Opacity="0.5">
      <Content/>
    </Frame>
  </Template>

  <Template Name="template:ImageListItem">
    <Control Id="NormalState">
      <Content/>
    </Control>
    <Frame Id="OverState" Config="frame:RoundedButton" Opacity="0.5" HiddenBorders="Middle">
      <Content/>
    </Frame>
    <Frame Id="DownState" Config="frame:RoundedButton" Opacity="0.5">
      <Content/>
    </Frame>
    <Frame Id="DownOverState" Config="frame:RoundedButtonOver" Opacity="0.5">
      <Content/>
    </Frame>
    <Frame Id="DownDisabledState" Config="frame:RoundedButtonDownDisabled" Opacity="0.5">
      <Content/>
    </Frame>
    <Frame Id="NormalDisabledState" Config="frame:RoundedButtonDisabled" Opacity="0.5">
      <Content/>
    </Frame>
  </Template>
</GUITemplates>